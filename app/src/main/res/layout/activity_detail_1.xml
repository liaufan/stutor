<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg3"
    tools:context=".Detail_1">

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginTop="130dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/indicator2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/airbnbcerealextrabold"
        android:text="Fill in your details"
        android:textColor="#3E3446"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbook"
        android:text="Tell us more about yourself"
        android:textColor="#3E3446"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbold"
        android:text="Full name"
        android:textColor="#719B98"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/fullName"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <EditText
        android:id="@+id/fullName"
        android:layout_width="285dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/square_button_stroke"
        android:ems="10"
        android:fontFamily="@font/airbnbcerealbook"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3E3446"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbold"
        android:text="Date of birth"
        android:textColor="#719B98"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/dateOfBirth"
        app:layout_constraintTop_toBottomOf="@+id/fullName" />

    <EditText
        android:id="@+id/dateOfBirth"
        android:layout_width="285dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/square_button_stroke"
        android:ems="10"
        android:fontFamily="@font/airbnbcerealbook"
        android:inputType="date"
        android:padding="15dp"
        android:textColor="#3E3446"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbold"
        android:text="Gender"
        android:textColor="#719B98"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/gender_spinner"
        app:layout_constraintTop_toBottomOf="@+id/dateOfBirth" />

    <Spinner
        android:id="@+id/gender_spinner"
        android:layout_width="285dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/square_button_stroke"
        android:padding="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbold"
        android:text="Current occupation"
        android:textColor="#719B98"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/currentOccupation"
        app:layout_constraintTop_toBottomOf="@+id/gender_spinner" />

    <EditText
        android:id="@+id/currentOccupation"
        android:layout_width="285dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/square_button_stroke"
        android:ems="10"
        android:fontFamily="@font/airbnbcerealbook"
        android:inputType="textPersonName"
        android:padding="15dp"
        android:textColor="#3E3446"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/airbnbcerealbold"
        android:text="Email address"
        android:textColor="#719B98"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/email"
        app:layout_constraintTop_toBottomOf="@+id/currentOccupation" />

    <EditText
        android:id="@+id/email"
        android:layout_width="285dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/square_button_stroke"
        android:ems="10"
        android:fontFamily="@font/airbnbcerealbook"
        android:inputType="textEmailAddress"
        android:padding="15dp"
        android:textColor="#3E3446"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/next_button"
        android:layout_width="210dp"
        android:layout_height="45dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/gradient_rounded_button"
        android:fontFamily="@font/airbnbcerealextrabold"
        android:text="NEXT"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>